<h1>Public::Users#index</h1>
<p>Find me in app/views/public/users/index.html.erb</p>

<%= search_form_for @q do |f| %>
<%= f.search_field :name_cont, placeholder: "ユーザ検索" %>
<%= f.submit "検索" %>
<% end %>

<% @users.each do |user| %>
<%= link_to user_path(user.id) do %>
<%= user.name %>
<% end %>
<% end %>